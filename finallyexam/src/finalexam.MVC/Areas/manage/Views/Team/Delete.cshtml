@model Team


    <form method="post" enctype="multipart/form-data">
        <div class="card" style="width: 18rem;">
            <img src="~/uploads/teams/@Model.ImageUrl" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">@Model.FullName</h5>
                <p class="card-text">@Model.Profession</p>
               
            </div>
            <button  class="btn btn-danger">Delete</button>
            <a asp-action="index" asp-controller="team" class="btn btn-secondary">go to home</a>
          
        </div>
    </form>
   

