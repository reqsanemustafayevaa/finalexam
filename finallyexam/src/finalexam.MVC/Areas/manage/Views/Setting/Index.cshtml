@model List<Setting>


@{
    int counter = 0;
}
<div class="container-fluid">
    <div d-flex justify-content-center align-items-between>
        <h1>setting</h1>
      
    </div>

    <table class="table">

        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">key</th>
                <th scope="col">Phone</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var setting in Model)
            {
                counter++;
                <tr>
                    <th scope="row">@counter</th>
                    <td>@setting.Key</td>
                    <td>@setting.Value</td>
                    
                    <td>
                        <a asp-action="update" asp-controller="setting" asp-route-id="@setting.Id" class="btn btn-success">Update</a>
                       

                    </td>


                </tr>
            }

        </tbody>
    </table>
</div>
